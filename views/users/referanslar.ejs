<!DOCTYPE html>
<html lang="tr">
<head>
    <%- include('../partials/head') %>
</head>
<body>
<!-- Navbar -->
<%- include('../partials/nav') %>

<!-- Ana İçerik -->
<div class="container mt-3">
    <div class="row">
        <div class="col-md-3">
            <div class="list-group">
                <%- include('../partials/link-panel',linkbar) %>
            </div>
            <img src="\static\images\asan-logo.jpeg" alt="" class="img-fluid">
        </div>
        <div class="col-md-9">
            <% referanslar.forEach(element=>{%>
                <div class="card mb-3">
                    <div class="row">
                        <div class="col-md-3">
                            <img src="\static\images\<%=element.referansimage%>.jpeg" alt="" class="img-fluid">
                        </div>
                        <div class="col-md-9">
                            <div class="card-body">
                                <h5 class="card-title"><%= element.referanstitle %></h5>
                                <p><%= element.referansmodel %></p>
                            </div>
                        </div>
                    </div>
                </div>
            <%})%>
            <!-- pagination -->
            <%- include('../partials/pagination') %>
        </div>
    </div>
</div>

<!-- Footer -->
<%- include('../partials/footer') %>

</body>
</html>